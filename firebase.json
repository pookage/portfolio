{
  "hosting": {
    "headers": [
      { "source":"/index.html", "headers": [{"key": "Cache-Control", "value": "no-cache"}] }
    ],
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "!/@(assets|Assets)/**",
        "destination": "/index.html"
      }
    ]
  }
}


    